{
    "version": "0.1.0",
    "configurations": [
      {
        "type": "node",
        "request": "attach",
        "name": "Attach to dev:debug",
        "protocol": "inspector",
        "port": 4321,
        "restart": true,
        "cwd": "${workspaceRoot}"
      },
      {
        "type": "node",
        "request": "launch",
        "name": "Debug",
        "protocol": "inspector",
        "cwd": "${workspaceRoot}",
        "runtimeExecutable": "npm",
        "runtimeArgs": ["run-script", "dev"]
      }
    ]
  }